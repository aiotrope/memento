{
  "name": "memento",
  "version": "0.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "start": "NODE_ENV=production node ./dist/index",
    "dev": "NODE_ENV=development nodemon --exec babel-node ./backend/index",
    "build": "rm -rf dist/ && babel ./backend --out-dir dist --copy-files",
    "eslint": "eslint --ignore-path .eslintignore",
    "lint": "yarn run eslint --fix",
    "prettier": "prettier --ignore-path .prettierignore --check \"**/*.+(js|ts|jsx|json)\"",
    "format": "yarn run prettier --write"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^6.0.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "sanitize": "^2.1.2",
    "sequelize": "^6.29.3",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/node": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.21.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.6"
  }
}
